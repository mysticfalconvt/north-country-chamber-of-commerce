{
  "name": "north-country-chamber-of-commerce",
  "version": "1.0.0",
  "description": "Website template for Payload",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "postbuild": "next-sitemap --config next-sitemap.config.cjs",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "dev:prod": "cross-env NODE_OPTIONS=--no-deprecation rm -rf .next && pnpm build && pnpm start",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "ii": "cross-env NODE_OPTIONS=--no-deprecation pnpm --ignore-workspace install",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,scss,md}\" && pnpm lint:fix",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,scss,md}\"",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation eslint .",
    "lint:fix": "cross-env NODE_OPTIONS=--no-deprecation eslint --fix .",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "reinstall": "cross-env NODE_OPTIONS=--no-deprecation rm -rf node_modules && rm pnpm-lock.yaml && pnpm --ignore-workspace install",
    "seed": "cross-env NODE_OPTIONS=--no-deprecation NODE_ENV=development tsx src/scripts/seed.ts",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "test": "pnpm run test:int && pnpm run test:e2e",
    "test:e2e": "cross-env NODE_OPTIONS=\"--no-deprecation --no-experimental-strip-types\" pnpm exec playwright test --config=playwright.config.ts",
    "test:int": "cross-env NODE_OPTIONS=--no-deprecation vitest run --config ./vitest.config.mts",
    "translate:backfill": "cross-env NODE_OPTIONS=--no-deprecation tsx src/scripts/backfill-translations.ts"
  },
  "dependencies": {
    "@payloadcms/admin-bar": "3.69.0",
    "@payloadcms/db-postgres": "3.69.0",
    "@payloadcms/email-nodemailer": "^3.69.0",
    "@payloadcms/live-preview-react": "3.69.0",
    "@payloadcms/next": "3.69.0",
    "@payloadcms/plugin-form-builder": "3.69.0",
    "@payloadcms/plugin-nested-docs": "3.69.0",
    "@payloadcms/plugin-redirects": "3.69.0",
    "@payloadcms/plugin-search": "3.69.0",
    "@payloadcms/plugin-seo": "3.69.0",
    "@payloadcms/richtext-lexical": "3.69.0",
    "@payloadcms/ui": "3.69.0",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@stripe/stripe-js": "^8.6.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "date-fns": "^4.1.0",
    "dotenv": "16.6.1",
    "geist": "^1.5.1",
    "graphql": "^16.12.0",
    "lucide-react": "^0.562.0",
    "next": "15.5.9",
    "next-sitemap": "^4.2.3",
    "nodemailer": "^7.0.12",
    "ol": "^10.7.0",
    "openai": "^6.15.0",
    "payload": "3.69.0",
    "prism-react-renderer": "^2.4.1",
    "react": "19.2.3",
    "react-day-picker": "^9.4.3",
    "react-dom": "19.2.3",
    "react-hook-form": "7.69.0",
    "react-ol": "^0.1.12",
    "sharp": "0.34.5",
    "stripe": "^20.1.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@playwright/test": "1.57.0",
    "@tailwindcss/typography": "^0.5.19",
    "@testing-library/react": "16.3.1",
    "@types/escape-html": "^1.0.4",
    "@types/node": "22.19.3",
    "@types/nodemailer": "^7.0.4",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react": "4.7.0",
    "autoprefixer": "^10.4.23",
    "copyfiles": "^2.4.1",
    "eslint": "^9.39.2",
    "eslint-config-next": "15.5.9",
    "jsdom": "26.1.0",
    "playwright": "1.57.0",
    "playwright-core": "1.57.0",
    "postcss": "^8.5.6",
    "prettier": "^3.7.4",
    "tailwindcss": "^3.4.19",
    "tsx": "^4.21.0",
    "typescript": "5.9.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.4"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^9 || ^10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp",
      "esbuild",
      "unrs-resolver"
    ]
  }
}
